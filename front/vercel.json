{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' http://localhost:3000;"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM http://localhost:3000"
          }
        ]
      }
    ]
  }