{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' http://localhost:3000 https://ia-chat-psi.vercel.app;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://ia-chat-psi.vercel.app"
        }
      ]
    }
  ]
}